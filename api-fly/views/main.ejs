         }

         .img-right img {
            height: 75vh;
         }
      }

      @media screen and (max-width: 768px) {
         .navbar {
            .nav-links {
               display: none;
            }

            .sidebar-btn {
               display: block;
            }
         }


         .container {
            padding-inline: 6vw;
         }

         .inside-img-section {
            flex-direction: column;
            gap: 1rem;
         }

         .img-left {
            width: 70%;
            overflow: hidden;
         }

         .img-left img {
            height: 60vh;
         }

         .img-right {
            width: 70%;
            overflow: hidden;
         }

         .img-right img {
            height: 60vh;
         }

         .footer {
            grid-template-columns: repeat(4, 1fr);

            .footer-section:nth-child(1) {
               grid-column: span 4;
            }

            .footer-section:nth-child(2),
            .footer-section:nth-child(3) {
               grid-column: span 2;
            }
         }
      }

      @media screen and (max-width: 480px) {
