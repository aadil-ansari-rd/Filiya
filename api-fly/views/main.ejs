   <style>
      * {
         margin: 0;
         padding: 0;
         box-sizing: border-box;
         font-family: "Roboto", sans-serif;
      }


      :root {
         --background-color: #fff;
         --background-light: #fff;
         --shadow-color: rgba(27, 0, 0, 0.5);
         --primary-color: rgba(255, 0, 86);
         --text-color: #0a0a0a;
         --text-light: #575757;
         --font-size: 17px;
         --animation-speed: 1;
      }

      .container {
         padding-inline: 18vw;
      }

      .navbar {
         height: 80px;
         box-shadow: 0 3px 5px var(--shadow-color);
         background-color: rgb(189, 185, 185);
         display: flex;
         justify-content: space-between;
         align-items: center;
         position: fixed;
         width: 100%;
         top: 0;
         z-index: 998;
      }

      .navbar .logo {
         font-size: 1.5em;
         font-weight: bolder;
         color: var(--primary-color);
         text-shadow: 0 1px 1.5px var(--shadow-color);
      }

      a {
         text-decoration: none;
      }

      .navbar .logo span {
         color: var(--text-color);
      }

      .navbar .nav-links a {
         margin-left: 1em;
         color: var(--text-light);
         letter-spacing: 2px;
         text-transform: uppercase;
         position: relative;
         font-size: 1em;
      }

      .navbar .nav-links a::before {
         content: "";
         display: inline-block;
         height: 3px;
         width: 100%;
         background-color: var(--primary-color);
         position: absolute;
         bottom: -5px;
         left: 0;
         transform: scaleX(0);
         transition: 0.2s ease-in-out;
      }


      /* .navbar .nav-links a.active {
 color: var(--primary-color);
 font-weight: bold;
} */

      .navbar .cart-logo {
         cursor: pointer;
      }

      .navbar .cart-logo span:nth-child(1) {
         font-size: 1.7rem;
         color: rgb(0, 94, 110);
      }

      .navbar .cart-logo span:nth-child(2) {
         padding: 0 3px;
         background-color: var(--primary-color);
         color: #000;
         font-weight: bold;
         border-radius: 5px;
         position: relative;
         top: -15px;
      }

      .navbar .sidebar-btn {
         height: 29px;
         width: 30px;
         position: relative;
         display: none;
      }

      .navbar .sidebar-btn .bar {
         border-radius: 5px;
         height: 6px;
         width: 100%;
         background-color: var(--text-color);
         position: absolute;
         transition: transform 0.5s;
      }

      .navbar .sidebar-btn .bar:nth-child(1) {
         top: 0;
      }

      .navbar .sidebar-btn .bar:nth-child(2) {
         top: 50%;
         transform-origin: right;
         transform: scaleX(0.8) translateY(-50%);
      }

      .navbar .sidebar-btn .bar:nth-child(3) {
         top: 100%;
         transform-origin: right;
         transform: scaleX(1.1) translateY(-100%);
      }

      .navbar .sidebar-btn.active .bar {
         transform-origin: center;
         top: 50%;
      }

      .navbar .sidebar-btn.active .bar:nth-child(1) {
         transform: translate(0, -50%) scaleX(1) rotate(45deg);
      }

      .navbar .sidebar-btn.active .bar:nth-child(2) {
         transform: scaleX(0) translateY(-50%);
      }

      .navbar .sidebar-btn.active .bar:nth-child(3) {
         transform: translate(0, -50%) scaleX(1) rotate(-45deg);
      }


      .inside-img-section {
         display: flex;
         justify-content: center;
         align-items: center;
         gap: 1%;
         width: 98%;
         padding-top: 8rem;
         padding-bottom: 2.5rem;
         overflow: hidden;

      }

      .img-left {
         width: 95%;
         overflow: hidden;
      }

      .img-left img {
         width: 100%;
         height: 65vh;
         border-radius: 8px;
         object-fit: cover;
         transition: .3s ease;
      }

      .img-left:hover img {
         transform: scale(1.1);
      }

      .img-right {
         width: 95%;
         overflow: hidden;
      }

      .img-right img {
         width: 100%;
         height: 65vh;
         object-fit: cover;
         border-radius: 8px;
         object-fit: cover;
         transition: .3s ease;
      }

      .img-right:hover img {
         transform: scale(1.1);
      }



      .footer {
         background-color: #0f0f0f;
         color: #fff;
         padding-block: 3em;
         display: grid;
         grid-template-columns: 2fr 1fr 1fr;
         grid-gap: 1em;

         .footer-section {
            p {
               opacity: 0.9;
               margin-bottom: 0.5em;
            }

            p.footer-title {
               opacity: 1;
               font-size: 1.1em;
               margin-bottom: 1em;
            }
         }
      }


      @media screen and (max-width: 1280px) {
         .container {
            padding-inline: 18vw;
         }
      }


      @media screen and (max-width: 1050px) {
         .container {
            padding-inline: 12vw;
         }
      }

      @media screen and (max-width: 900px) {
         .inside-img-section {
